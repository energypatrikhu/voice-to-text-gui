{
	"name": "voice-to-text",
	"version": "0.6.13",
	"private": true,
	"description": "Voice To Text",
	"main": "resources/app/electron.cjs",
	"type": "module",
	"author": "EnergyPatrikHU",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev concurrently --kill-others -n=svelte,electron -c='#ff3e00',blue \"vite dev\" \"node electron.esbuild.config.mjs && electron resources/app/electron.cjs\"",
		"build": "cross-env NODE_ENV=production node electron.esbuild.config.mjs && vite build && electron-builder -w --config build.config.json",
		"build:beta": "cross-env NODE_ENV=production APP_STATE=beta node electron.esbuild.config.mjs && vite build && electron-builder -w --config build.beta.config.json",
		"release": "cross-env-file -p .env.json release-it --config release.release-it.json",
		"release:beta": "cross-env-file -p .env.json release-it --config preRelease.release-it.json",
		"build:release": "npm run build && npm run release",
		"build:release:beta": "npm run build:beta && m run release:beta"
	},
	"browserslist": [
		"Chrome 89"
	],
	"dependencies": {
		"active-win": "^8.1.1",
		"electron-context-menu": "^3.6.1",
		"electron-reloader": "^1.2.3",
		"electron-serve": "^1.1.0",
		"electron-window-state": "^5.0.3",
		"highlight.js": "latest",
		"keysender": "^2.3.0",
		"lodash": "latest",
		"moment": "^2.29.4",
		"puppeteer-core": "^21.4.1",
		"uiohook-napi": "^1.5.2"
	},
	"devDependencies": {
		"@material-symbols/svg-400": "^0.13.2",
		"@poppanator/sveltekit-svg": "^4.1.3",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.27.2",
		"@types/dom-speech-recognition": "^0.0.3",
		"@types/lodash": "^4.14.200",
		"@types/node": "^20.8.10",
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"cross-env-file": "^1.0.0",
		"dotenv": "^16.3.1",
		"electron": "^27.0.2",
		"electron-builder": "^24.6.4",
		"electron-connect": "^0.6.3",
		"electron-packager": "^17.1.2",
		"electron-updater": "^6.1.4",
		"esbuild": "^0.19.5",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.31",
		"postcss-advanced-variables": "^3.0.1",
		"postcss-calc": "^9.0.1",
		"release-it": "^16.2.1",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"svelte-highlight": "^7.4.1",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.5",
		"tailwindcss-all": "^0.0.2",
		"terser": "^5.24.0",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	}
}
