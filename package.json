{
	"name": "voice-to-text",
	"version": "0.6.12",
	"private": true,
	"description": "Voice To Text",
	"main": "resources/app/electron.cjs",
	"type": "module",
	"author": "EnergyPatrikHU",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev concurrently --kill-others -n=svelte,electron -c='#ff3e00',blue \"vite dev\" \"node electron.esbuild.config.mjs && electron resources/app/electron.cjs\"",
		"build": "cross-env NODE_ENV=production node electron.esbuild.config.mjs && vite build && electron-builder -w --config build.config.json",
		"build:beta": "cross-env NODE_ENV=production APP_STATE=beta node electron.esbuild.config.mjs && vite build && electron-builder -w --config build.beta.config.json",
		"release": "release-it --config release.release-it.json",
		"release:beta": " release-it --config preRelease.release-it.json",
		"build:release": "npm run build && npm run release",
		"build:release:beta": "npm run build:beta && m run release:beta"
	},
	"browserslist": [
		"Chrome 89"
	],
	"dependencies": {
		"active-win": "latest",
		"electron-context-menu": "latest",
		"electron-reloader": "latest",
		"electron-serve": "latest",
		"electron-window-state": "latest",
		"highlight.js": "latest",
		"keysender": "latest",
		"lodash": "latest",
		"moment": "latest",
		"puppeteer-core": "latest",
		"uiohook-napi": "latest"
	},
	"devDependencies": {
		"@material-symbols/svg-400": "latest",
		"@poppanator/sveltekit-svg": "latest",
		"@sveltejs/adapter-static": "latest",
		"@sveltejs/kit": "latest",
		"@types/dom-speech-recognition": "latest",
		"@types/lodash": "latest",
		"@types/node": "latest",
		"autoprefixer": "latest",
		"concurrently": "latest",
		"cross-env": "latest",
		"dotenv": "latest",
		"electron": "latest",
		"electron-builder": "latest",
		"electron-connect": "latest",
		"electron-packager": "latest",
		"electron-updater": "latest",
		"esbuild": "latest",
		"npm-run-all": "latest",
		"postcss": "latest",
		"postcss-advanced-variables": "latest",
		"postcss-calc": "latest",
		"svelte": "latest",
		"svelte-check": "latest",
		"svelte-highlight": "latest",
		"svelte-preprocess": "latest",
		"tailwindcss": "latest",
		"tailwindcss-all": "latest",
		"terser": "latest",
		"tslib": "latest",
		"typescript": "latest",
		"vite": "latest"
	}
}
